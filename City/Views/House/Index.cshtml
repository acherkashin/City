<script type="text/javascript">
    function getHomes() {
        $.ajax({
            url: 'http://@Context.Request.Host/House/GetAll',
            type: 'GET',
            contentType: "application/json",
            success: function (homes) {
                var rows = "";
                $.each(homes,
                    function (index, home) {
                        rows += getRow(home);
                    });
                $("table tbody").append(rows);
            }
        });
    }
    function getRow(home) {
        return "<tr data-rowid='" + home.id + "'>"
            + "<td>" + home.id + "</td>"
            + "<td>" + home.name + "</td>"
            + "<td>" + home.electricMeter.spentPower + "</td>"
            + "<td>" + home.gasMeter.currentVolume + "</td>"
            + "<td>" + home.waterMeter.currentVolume + "</td>" +
            "<td><input class='switch-light' data-id="+home.id+" type='checkbox'></td></tr>";
    }

    getHomes();


</script>


